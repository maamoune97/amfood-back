{{form_start(form)}}

<div class="row">
    <div class="col-12 col-md-6 form-group">
        {{form_row(form.name)}}
    </div>
    <div class="col-12 col-md-6 form-group">
        {{form_row(form.location)}}
    </div>
</div>

<div class="row">
    <div class="col-12 col-md-6 ">
        {{form_row(form.phone)}}
    </div>
    <div class="col-12 col-md-6 ">
        {{form_row(form.email)}}
    </div>
</div>

<div class="row">
    <div class="col-12 col-md-6 ">
        {{form_row(form.imageLogo)}}
    </div>
    <div class="col-12 col-md-6">
        {{form_row(form.speciality)}}
    </div>
</div>

<div class="row">
    <div class="col-12 col-md-6 ">
        {{form_row(form.managers)}}
    </div>
    <div class="col-12 col-md-6 d-flex align-items-end">
        {{form_row(form.activate)}}
    </div>
</div>

<div class="float-right form-group">
    <button type="submit" class="btn btn-success">{{submitBtnLabel}}</button>
</div>

{{form_end(form)}}